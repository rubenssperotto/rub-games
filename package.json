{
  "name": "rub-games-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepare": "husky install",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@styled-icons/boxicons-regular": "^10.38.0",
    "@styled-icons/boxicons-solid": "^10.38.0",
    "@styled-icons/fa-brands": "^10.38.0",
    "@types/lodash.xor": "^4.5.7",
    "apollo": "^2.34.0",
    "apollo-client": "^2.6.10",
    "graphql": "^16.5.0",
    "lodash.xor": "^4.5.0",
    "next": "12.2.2",
    "next-pwa": "^5.3.1",
    "polished": "^4.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.29.0",
    "styled-components": "5.3.5",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@apollo/rover": "^0.7.0",
    "@graphql-codegen/cli": "2.8.1",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/typed-document-node": "^2.3.1",
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "^2.5.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.1",
    "@graphql-codegen/typescript-react-query": "^3.6.1",
    "@graphql-codegen/typescript-resolvers": "2.7.1",
    "@storybook/addon-essentials": "6.4.17",
    "@storybook/builder-webpack5": "^6.4.17",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "6.4.17",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.27",
    "@types/react-slick": "^0.23.10",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.2.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^12.3.3",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "storybook-addon-next-router": "^3.0.8",
    "typescript": "^4.5.5",
    "webpack": "5.68.0"
  }
}
